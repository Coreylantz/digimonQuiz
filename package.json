{
  "name": "digimon-quiz",
  "version": "0.0.0",
  "description": "A quiz to find your digital partner.",
  "main": "",
  "scripts": {
    "start": "node server.js",
    "dev-build": "webpack -d --config webpack.config.dev",
    "build": "webpack -p --config webpack.config.production",
    "test": "xo",
    "get-info": "node ./tools/get-digimon-info.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Coreylantz/digimonQuiz.git"
  },
  "author": [
    "Dan E",
    "Corey Lantz"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Coreylantz/digimonQuiz/issues"
  },
  "homepage": "https://github.com/Coreylantz/digimonQuiz#readme",
  "xo": {
    "envs": [
      "node",
      "browser"
    ],
    "space": true,
    "extends": "xo-react",
    "rules": {
      "react/jsx-space-before-closing": [
        2,
        "always"
      ],
      "import/no-extraneous-dependencies": 0
    }
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.25.0",
    "eslint-config-xo-react": "^0.10.0",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.10.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1",
    "xo": "^0.17.0",
    "jquery": "^3.1.1",
    "jsdom": "^9.5.0",
    "superagent": "^2.3.0"
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "immutable-js": "^0.3.1-6",
    "immutable-props": "^1.1.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.5"
  }
}
